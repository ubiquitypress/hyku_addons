<!-- _<%= key %> -->
<% if f.object.schema_driven? %>
  <%= render "records/edit_fields/default", f: f, key: key %>

<% else %>
  <% qualification_level_service = HykuAddons::QualificationLevelService.new %>
  <% selected = f.object[:qualification_level].empty? ? nil : f.object[:qualification_level] %>
  <% model = f.object.model.class.to_s.underscore %>

  <% options = {
    f: f,
    model: model,
    qualification_service: qualification_level_service,
    field_name: "qualification_level",
    css_class_name: "qualification-level"
  } %>

  <%= render "hyku_addons/qualification/qualification", options %>
<% end %>
