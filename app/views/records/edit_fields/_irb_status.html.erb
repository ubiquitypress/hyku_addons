<% # This file can be removed when the schema migration has been completed %>
<!-- _irb_status -->
<% if f.object.schema_driven? %>
  <%= render "records/edit_fields/default", f: f, key: key %>

<% else %>
<%= f.input :irb_status, as: :select,
  collection: HykuAddons::IrbStatusService.new(model: f.object.model.class).select_active_options,
  include_blank: true,
  input_html: { class: 'form-control' } %>
<% end %>

